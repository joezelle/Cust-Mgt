@model CustomerMgt.MVC.Models.ResponseModel<string>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <div>
        <h1>An error occurred</h1>
        <p>@Model.Message</p>
        <p>Error Code: @Model.ResponseCode</p>
        <p>Request Successful: @Model.RequestSuccessful</p>
    </div>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</body>
</html>
